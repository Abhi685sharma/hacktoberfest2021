// print the subsequence of string
#include <iostream>
#include <string>
using namespace std;
int sub_sequence(string input, string output[])
{
    if (input.empty())
    {
        output[0] = "";
        return 1;
    }
    string smalloutput = input.substr(1);
    int ans = sub_sequence(smalloutput, output);
    for (int i = 0; i < ans; i++)
    {
        output[i + ans] = input[0] + output[i];
    }
    
    return 2 * ans;
}
int main()
{
    string input = "abcd";
    string *output = new string[1000];
    int a = sub_sequence(input,output);
    for (int i = 0; i < a; i++)
    {
        cout << output[i] << endl;
    }
    return 0;
}
